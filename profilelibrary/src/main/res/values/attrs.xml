<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProfileView">
        <attr name="image" format="reference" />
        <attr name="username" format="string" />
        <attr name="email" format="string" />
        <attr name="number" format="string" />
    </declare-styleable>

    <declare-styleable name="PulseLoader">
        <attr name="barColor" format="color" />
    </declare-styleable>

    <declare-styleable name="FillButton">
        <attr name="backColor" format="color" />
        <attr name="frontColor" format="color" />
    </declare-styleable>

    <declare-styleable name="BalanceMeter">
        <attr name="meterBackColor" format="color" />
        <attr name="meterFrontColor" format="color" />
        <attr name="balanceTextColor" format="color" />
        <attr name="remTextColor" format="color" />
        <attr name="limitTextColor" format="color" />
        <attr name="meterWidth" format="dimension" />
        <attr name="balanceTextSize" format="dimension" />
        <attr name="remTextSize" format="dimension" />
        <attr name="limitTextSize" format="dimension" />
    </declare-styleable>
</resources>